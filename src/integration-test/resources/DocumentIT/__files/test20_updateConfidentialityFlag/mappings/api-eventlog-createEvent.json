{
	"name": "api-eventlog-createEvent",
	"request": {
		"method": "POST",
		"urlPath": "/api-eventlog/00000000-0000-0000-0000-000000000000",
		"headers": {
			"Authorization": {
				"equalTo": "Bearer 0000000000000000AAAAAAAAAAAAAAAA"
			}
		},
		"bodyPatterns": [
			{
				"ignoreArrayOrder": true,
				"equalToJson": {
					"type": "UPDATE",
					"message": "Confidentiality flag updated to: 'true' with legal citation: '25 kap. 1 ยง OSL' for document with registrationNumber: '2023-2281-123'. Action performed by: 'username123'",
					"expires": "${json-unit.any-string}",
					"owner": "Document",
					"metadata": [
						{
							"key": "ExecutedBy",
							"value": "username123"
						},
						{
							"key": "RegistrationNumber",
							"value": "2023-2281-123"
						}
					]
				}
			}
		]
	},
	"response": {
		"headers": {
			"Content-Type": "application/json"
		},
		"status": 202
	}
}